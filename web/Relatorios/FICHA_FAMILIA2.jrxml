	<queryString><![CDATA[SELECT pac.*,
 CASE WHEN fl_alc='S' THEN 'ALC' ELSE''END  ||
 CASE WHEN fl_cha='S' THEN 'CHA' ELSE''END  ||
 CASE WHEN fl_def='S' THEN 'DEF' ELSE''END  ||
 CASE WHEN fl_dia='S' THEN 'DIA' ELSE''END  ||
 CASE WHEN fl_dme='S' THEN 'DME' ELSE''END  ||
 CASE WHEN fl_epi='S' THEN 'EPI' ELSE''END  ||
 CASE WHEN fl_ges='S' THEN 'GES' ELSE''END  ||
 CASE WHEN fl_han='S' THEN 'HAN' ELSE''END  ||
 CASE WHEN fl_ha='S' THEN 'HA' ELSE''END  ||
 CASE WHEN fl_mal='S' THEN 'MAL' ELSE''END  ||
 CASE WHEN fl_tbc='S' THEN 'TBC' ELSE''END  
 as cd_doencas,ds_ocupacao FROM 
PACFAMILIA pac left join ocupacao ocu on ocu.cd_ocupacao=pac.cd_ocupacao
where cd_familia = $P{cd_familia}
and fl_menorquinze='S' and cd_agente=$P{cd_agente}]]></queryString>

